#include "move.nxc"

task main(){
  SetSensorUltrasonic(IN_1);
    while(true){
        if(SensorUS(IN_1) < 15){
            Off(OUT_AC);
            break;
        }
        else {
            goAhead(4);
        }
    }
    while(true){
        if(SensorUS(IN_1) > 25){
            Off(OUT_AC);
            break;
        }
        else {
             turnRight(3);
        }
    }
    goAhead(20);
}
